{
  "Chi": {
    "prefix": "chi-square",
    "body": "\\Chi_{${1:\\gamma}}^2"
  },
  "temp": {
    "prefix": "ee",
    "body": "\\e^{$1} $0"
  },
  "ww": {
    "prefix": "ww",
    "body": "\\omega_0 "
  },
  "fourier-transform": {
    "prefix": "fourier",
    "body": "\\frac{1}{\\sqrt{2\\pi}} \\int_{\\RR} \\e^{-i x \\xi} $1 \\d x "
  },
  "xor": {
    "prefix": "xor",
    "body": "\\oplus "
  },
  "operatorname": {
    "prefix": "ope",
    "body": "\\operatorname{$1} "
  },
  "question": {
    "prefix": [
      "problem",
      "question"
    ],
    "body": [
      "%----------------------------------------------------------------------------------------",
      "%\tQUESTION $1",
      "%----------------------------------------------------------------------------------------",
      "\\begin{question}[]{}",
      "\t\\begin{answer}",
      "\t\\end{answer}",
      "\\end{question}"
    ]
  },
  "answer": {
    "prefix": "answer",
    "body": [
      "\\begin{answer}",
      "\t$0",
      "\\end{answer}"
    ]
  },
  "subquestion": {
    "prefix": [
      "subquestion",
      "squestion"
    ],
    "body": [
      "\\begin{subquestion}[]",
      "\t\\begin{answer}",
      "\t\\end{answer}",
      "\\end{subquestion}"
    ]
  },
  "subsubquestion": {
    "prefix": [
      "subsubquestion",
      "ssquestion"
    ],
    "body": [
      "\\begin{subsubquestion}[]",
      "\t\\begin{answer}",
      "\t\\end{answer}",
      "\\end{subsubquestion}"
    ]
  },
  "Math": {
    "prefix": "PP",
    "body": "\\mathbb{P}"
  },
  "big-bracket": {
    "prefix": "big-bracket",
    "body": [
      "\\left\\{",
      "\t\\begin{array}{ll}",
      "\t\t$0",
      "\t\\end{array}",
      "\\right."
    ]
  },
  "color-RGB": {
    "prefix": "color-RGB",
    "body": "\\textcolor[RGB]{$1}{${2:$TM_SELECTED_TEXT}}$0",
    "description": "The range is [0, 255]"
  },
  "color": {
    "prefix": "color",
    "body": "\\textcolor{${1:green}}{${2:$TM_SELECTED_TEXT}}$0"
  },
  "box text": {
    "prefix": "box-text",
    "body": [
      "\\fbox{${0:$TM_SELECTED_TEXT}}"
    ]
  },
  "box equation": {
    "prefix": "box-equation",
    "body": "\\boxed{${0:$TM_SELECTED_TEXT}}"
  },
  "box-with-line break": {
    "prefix": "boxWithLineBreak",
    "body": [
      "\\fbox{\\parbox{\\textwidth}{%",
      "\t${0:$TM_SELECTED_TEXT}",
      "}}"
    ]
  },
  "lstset": {
    "prefix": "lstset",
    "body": [
      "\\lstset{",
      "\tbasicstyle       = \\ttfamily,           % 基本代码风格",
      "\tkeywordstyle     = \\color{purple}\\bfseries,   % 关键字风格",
      "\tidentifierstyle  = \\color{brown!80!black},",
      "\tcommentstyle     = \\rmfamily\\itshape,   % 注释的风格，斜体",
      "\tstringstyle      = \\ttfamily,           % 字符串风格",
      "\tflexiblecolumns,                        % 别问为什么，加上这个",
      "\tnumbers          = left,                % 行号的位置在左边",
      "\tshowspaces       = false,               % 是否显示空格，显示了有点乱，所以不现实了",
      "\tnumberstyle      = \\ttfamily\\color[RGB]{0,96,96}, % 行号的样式，小五号，tt等宽字体",
      "\tshowstringspaces = false,",
      "\tcaptionpos       = t,                   % 这段代码的名字所呈现的位置，t指的是top上面",
      "\tframe            = lrtb,                % 显示边框",
      "\tbreaklines=true,",
      "\tpostbreak=\\mbox{\\textcolor{red}{$\\hookrightarrow$}\\space},",
      "}"
    ]
  },
  "lstdefinestyle": {
    "prefix": "lstdefinestyle",
    "body": [
      "\\lstdefinestyle{Python}{",
      "\tlanguage        =   Python, % 语言选Python",
      "\tbasicstyle      =   \\ttfamily,",
      "\tnumberstyle     =   \\ttfamily,",
      "\tkeywordstyle    =   \\color{blue},",
      "\tkeywordstyle    =   [2] \\color{teal},",
      "\tstringstyle     =   \\color{magenta},",
      "\tcommentstyle    =   \\color{red}\\ttfamily,",
      "\tbreaklines      =   true,   % 自动换行，建议不要写太长的行",
      "\tcolumns         =   fixed,  % 如果不加这一句，字间距就不固定，很丑，必须加",
      "\tbasewidth       =   0.5em,",
      "}"
    ]
  },
  "template-math": {
    "prefix": "template",
    "body": [
      "\\documentclass[a4paper]{article}",
      "",
      "\\usepackage[utf8]{inputenc}",
      "\\usepackage[T1]{fontenc}",
      "",
      "%\\setlength{\\parskip}{0.5\\baselineskip}",
      "",
      "\\usepackage{geometry}",
      "\\geometry{left = 2.54 cm, right = 2.54 cm, top = 2.54 cm, bottom = 2.54 cm}",
      "",
      "\\usepackage{setspace}",
      "\\renewcommand{\\baselinestretch}{1.0}",
      "\\usepackage{indentfirst}",
      "\\setlength{\\parindent}{2em}",
      "",
      "%\\usepackage{fontspec}",
      "%\\setmainfont{Times New Roman}",
      "",
      "\\PassOptionsToPackage{hyphens}{url}",
      "\\usepackage{hyperref}",
      "\\usepackage{ulem}",
      "\\usepackage{graphicx}",
      "%\\usepackage{wrapfig}",
      "\\usepackage{enumitem}",
      "%\\usepackage{xcolor}",
      "%\\usepackage{subcaption}",
      "\\usepackage{float}",
      "\\usepackage{amsmath, amssymb, amsthm}",
      "\\usepackage{booktabs}",
      "",
      "%\\pagestyle{empty} % Not showing page number",
      "",
      "\\newcommand{\\TODO}[1]{\\textcolor{red}{TODO\\@: #1} }",
      "",
      "\\renewcommand{\\thesection}{\\Roman{section}}",
      "\\renewcommand{\\thesubsection}{\\Alph{subsection}}",
      "\\renewcommand{\\thesubsubsection}{\\thesubsection.\\arabic{subsubsection}}",
      "\\renewcommand{\\d}{\\: \\mathrm{d} }",
      "\\newcommand{\\e}{\\mathrm{e}}",
      "",
      "\\begin{document}",
      "",
      "\t$0",
      "",
      "\\end{document}"
    ]
  },
  "subfigure/subcaption": {
    "prefix": [
      "subcaption",
      "subfigure"
    ],
    "body": [
      "\\begin{figure}[H]",
      "\t\\centering",
      "\t\\begin{subfigure}[b]{0.45\\linewidth}",
      "\t\t\\centering",
      "\t\t\\includegraphics[width=0.9\\linewidth]{$1}",
      "\t\t\\caption{$2}",
      "\t\t\\label{subfig:${3:$1}}",
      "\t\\end{subfigure}",
      "\t\\begin{subfigure}[b]{0.45\\linewidth}",
      "\t\t\\centering",
      "\t\t\\includegraphics[width=0.9\\linewidth]{$4}",
      "\t\t\\caption{$5}",
      "\t\t\\label{subfig:${6:$4}}",
      "\t\\end{subfigure}",
      "\t\\caption{$7}",
      "\t\\label{fig:$8}",
      "\\end{figure}",
      "$0"
    ],
    "description": "Use 'subcaption' package first"
  },
  "subfigure simple": {
    "prefix": "subfigure simple",
    "body": [
      "\\begin{subfigure}[b]{0.45\\linewidth}",
      "\t\\centering",
      "\t\\includegraphics[width=0.9\\linewidth]{$1}",
      "\t\\caption{$2}",
      "\t\\label{subfig:${3:$1}}",
      "\\end{subfigure}",
      "$0"
    ]
  },
  "enumerate": {
    "prefix": "enumerate",
    "body": [
      "\\begin{enumerate}[label = {${1:\\arabic*.}}]",
      "\t\\item $0",
      "\\end{enumerate}"
    ]
  },
  "itemize": {
    "prefix": "itemize",
    "body": [
      "\\begin{itemize}[leftmargin = 0.5cm]",
